{
"name": "Tab Organizer",
"version": "2.0",
"manifest_version": 2,

"description": "Categorize your tabs",

"content_scripts": [{
"matches": ["<all_urls>"],
  "js": ["jquery-3.2.1.js", "jquery-ui.js", "jquery-ui.min.js"],
  "run_at": "document_end"
}],

"browser_action": {
  "default_title": "Organize your tabs",
  "default_popup": "popup.html",
  "default_icon": "icon.png"
},

"permissions": ["tabs"]
}
